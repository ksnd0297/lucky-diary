{
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "extends": [
    "plugin:@typescript-eslint/recommended",
    "airbnb",
    "airbnb/hooks",
    "next/core-web-vitals",
    "prettier"
  ],
  "rules": {
    "react/jsx-filename-extension":["off",{"extensions":[".tsx"]} ],
    "react/jsx-props-no-spreading": ["off"],
    "no-tabs": 0
  }
}
